{
    "version": 2,
    "name": "Hoja de vida Mario Esteban Ortega GarcÃ©s",
    "builds": [
      { "use": "@now/static-build", "src": "package.json"}
    ],
    "routes": [
      {
        "src": "(.*).js", "dest": "/$1.js"
      },
      {
        "src": "(.*).json", "dest": "/$1.json"
      },
      {
        "src": "/.*", "dest": "/index.html"
      },
      {
        "src": "/public*", "dest": "/public"
      }
      {
        "src": "/css*", "dest": "/css"
      }
    ]
  }