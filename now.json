{
    "version": 2,
    "name": "Hoja de vida Mario Esteban Ortega Garc√©s",
    "builds": [
      { "use": "@now/static-build", "src": "package.json"}
    ],
    "routes": [
      {
        "src": "(.*).js", "dest": "/$1.js"
      },
      {
        "src": "(.*).json", "dest": "/$1.json"
      },
      {
        "src": "/.*", "dest": "/index.html"
      }
    ]
  }